{
  "name": "vue-ts-quasar",
  "version": "0.0.1",
  "description": "a front-end solution based on Vue 3 with TS in Quasar",
  "productName": "Quasar TS Admin Template",
  "author": "psi <cy.lonsido@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron -- --no-sandbox --disable-setuid-sandbox",
    "build": "quasar clean && quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:unit": "vitest --config './vitest/vitest.config.ts'",
    "coverage": "vitest run --coverage --config './vitest/vitest.config.ts'"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.5",
    "@vueuse/core": "^8.2.5",
    "axios": "^0.26.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "quasar": "^2.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.11",
    "@quasar/app-vite": "^1.0.0-beta.13",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.22",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vue/test-utils": "^2.0.0-rc.19",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "electron": "^18.0.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.6.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.6.9",
    "vitest": "^0.9.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}

{
  "name": "vue-ts-quasar",
  "version": "0.0.1",
  "description": "a front-end solution based on Vue 3 with TS in Quasar",
  "productName": "Quasar TS Admin Template",
  "author": "psi <cy.lonsido@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron -- --no-sandbox --disable-setuid-sandbox",
    "build": "quasar clean && quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:unit": "vitest --config ./vitest/vitest.config.ts",
    "coverage": "vitest run --coverage --config ./vitest/vitest.config.ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.5",
    "@vueuse/core": "^9.3.0",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "quasar": "^2.8.4",
    "vue": "^3.2.33",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@quasar/app-vite": "^1.1.2",
    "@types/lodash": "^4.14.181",
    "@types/node": "^18.7.23",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "cors": "^2.8.5",
    "cypress": "^10.9.0",
    "dotenv": "^16.0.0",
    "electron": "^21.0.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.11.2",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": "^16 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
